@model IEnumerable<BookBooking.Models.User>

<div class="container">
    <div class="row">
        @*TODO: 検索ボックス*@

        <div class="col">
            <div class="list-group">
                @foreach (var item in Model)
                {
                    <a asp-action="Edit" asp-route-id="@item.Id" class="list-group-item list-group-item-action d-flex justify-content-between align-items-center">
                        @item.Name
                    </a>
                }
            </div>
        </div>
    </div>
</div>
